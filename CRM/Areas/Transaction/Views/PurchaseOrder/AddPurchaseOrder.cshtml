
@{
    ViewBag.Title = "AddPurchaseOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string TempImg = System.Configuration.ConfigurationManager.AppSettings["TempImgPath"];
    string POMuchineImagePath = System.Configuration.ConfigurationManager.AppSettings["POProductImagePath"];
    string POProductImagePath = System.Configuration.ConfigurationManager.AppSettings["POProductImagePath"];
}

<style>
    ul.tab {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
        border: 1px solid #ccc;
        background-color: #f1f1f1;
    }

        /* Float the list items side by side */
        ul.tab li {
            float: left;
        }

            /* Style the links inside the list items */
            ul.tab li a {
                display: inline-block;
                color: black;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
                transition: 0.3s;
                font-size: 11px;
                font-weight: bold;
            }

                /* Change background color of links on hover */
                ul.tab li a:hover {
                    background-color: #ddd;
                }

                /* Create an active/current tablink class */
                ul.tab li a:focus, .active {
                    background-color: #ccc;
                }

    /* Style the tab content */
    .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #ccc;
        border-top: none;
    }
</style>

<div data-ng-controller="PurchaseOrderCtrl">
    <div class="row" data-ng-init="SetPurchaseOrderId(@ViewBag.id,@ViewBag.isdisable)">
        <div class="col-sm-12">
            <div class="box">
                <div class="box-body no-padding">
                    <form accessible-form role="form" name="FormPurchaseOrder" id="FormPurchaseOrder" ng-class="{'submitted': submitted}" ng-submit="submitted = true" novalidate>
                        <fieldset ng-disabled="isClicked">
                            <div class="box box-info">
                                <div class="box-header with-border">
                                    <h3 class="box-title">
                                        Purchase Order Information
                                    </h3>
                                    <span><a href="~/Transaction/PurchaseOrder"> <i class="fa fa-chevron-left"></i>Back</a></span>
                                </div>
                            </div>

                            <ul class="tab">
                                <li><a ng-class="{active : tabname == 'BasicDetails'}" data-ng-click="openTab(event, 'BasicDetails')">Basic Details</a></li>
                                <li><a ng-class="{active : tabname == 'ProductDetails'}" data-ng-click="openTab(event, 'ProductDetails',objPurchaseOrder)">Product Details</a></li>
                                @*<li><a ng-class="{active : tabname == 'PhotosDetails'}" data-ng-click="openTab(event, 'PhotosDetails',objPurchaseOrder)">Photos Details</a></li>*@
                                <li>@*<a ng-class="{active : tabname == 'TechnicalspecificationDetail'}" data-ng-click="openTab(event, 'TechnicalspecificationDetail',objPurchaseOrder)">Technical Spec Details</a>*@</li>
                                @*<li><a ng-class="{active : tabname == 'AccountDetails'}" data-ng-click="openTab(event, 'AccountDetails',objPurchaseOrder)">Account Details</a></li>*@
                                <li><a ng-class="{active : tabname == 'ShipmentDetails'}" data-ng-click="openTab(event, 'ShipmentDetails',objPurchaseOrder)">Shipment Details</a></li>
                                @*<li><a ng-class="{active : tabname == 'TaxationDetail'}" data-ng-click="openTab(event, 'TaxationDetail')">Taxation Details</a></li>*@
                                <li><a ng-class="{active : tabname == 'TermsDetails'}" data-ng-click="openTab(event, 'TermsDetails',objPurchaseOrder)">Terms Details</a></li>
                            </ul>

                            <div class="box-body">

                                <div id="BasicDetails" class="panel panel-primary tabcontent">
                                    <div class="panel-body">
                                        <div class="row">
                                            
                                            @*<div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label for="PoRefNo" class="control-label">Reference No<span class="text-danger">*</span> </label>
                                                        <input type="text" class="form-control" id="PoRefNo" disabled name="PoRefNo" data-ng-model="objPurchaseOrder.PoRefNo" placeholder="Reference No" maxlength="50" required tabindex="2">
                                                        <span class="errorMsg" ng-show="submitted && FormPurchaseOrder.PoRefNo.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Purchase Order Referance No is required.</span>
                                                    </div>
                                                </div>*@
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label for="Company" class="control-label">Company :<b class="errorMsg">*</b></label>
                                                    <elaunch-autocomplete min-chars="0" mode="CompanyMaster" id="Company" isdisplay="true"
                                                                          placeholder="Enter Company" tabindex="1"
                                                                          value-changed="callback(value)" ng-model="objPurchaseOrder.CompanyData">
                                                    </elaunch-autocomplete>
                                                    <span class="errorMsg" ng-show="submitted && objPurchaseOrder.CompanyData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Company Name is required.</span>
                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label for="PoNo" class="control-label">Purchase Order No@*<span class="text-danger">*</span>*@ </label>
                                                    <input type="text" class="form-control" id="PoNo" name="PoNo" data-ng-model="objPurchaseOrder.PoNo" placeholder="Purchase Order No" maxlength="50" required tabindex="1" readonly>
                                                    <span class="errorMsg" ng-show="submitted && FormPurchaseOrder.PoNo.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Purchase Order No is required.</span>
                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label for="PoDate" class="control-label">Purchase Order Date<span class="text-danger">*</span> </label>
                                                    <elaunch-datepicker name="PoDate" ng-model="objPurchaseOrder.PoDate" date-options="dateOptions" week-off="0" tabindex="3" ng-required="true"></elaunch-datepicker>
                                                    <span class="errorMsg" ng-show="submitted && objPurchaseOrder.PoDate == null || FormPurchaseOrder.PoDate.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Purchase Order Date is required.</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-sm-6">
                                                <h4><b>Buyer Details</b></h4>
                                            </div>
                                            <div class="col-sm-6">
                                                <h4><b>Supplier Details</b></h4>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label for="Company" class="control-label">Company Name :<b class="errorMsg">*</b></label>
