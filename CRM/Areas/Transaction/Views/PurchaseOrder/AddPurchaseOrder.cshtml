                                                </div>
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="form-group">
                                                    <label for="Description" class="control-label">Description</label>
                                                    <textarea class="form-control" id="Description" name="Description" data-ng-model="objPurchaseOrderDetail.Description" placeholder="Description" rows="2" cols="60" style="max-width: 100%;"></textarea>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <div class="form-group">
                                                    <label for="Qty" class="control-label">Qty<span class="text-danger">*</span> </label>
                                                    <input type="text" oninput="this.value = this.value.replace(/[^0-9.]/g, ''); this.value = this.value.replace(/(\..*)\./g, '$1');" class="form-control" id="Qty" name="Qty" data-ng-model="objPurchaseOrderDetail.Qty" placeholder="Qty" maxlength="5" required>
