                                                                          value-changed="callback(value)" ng-model="objPurchaseOrderDetail.QtyCodeValueData">
                                                    </elaunch-autocomplete>
                                                    <span class="errorMsg" ng-show="PODetsubmitted && (objPurchaseOrderDetail.QtyCodeValueData.Display == '' || objPurchaseOrderDetail.QtyCodeValueData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Qty Code is required.</span>
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="form-group">
                                                    <label for="UnitPrice" class="control-label">Unit Price<span class="text-danger">*</span> </label>
                                                    <input type="text" oninput="this.value = this.value.replace(/[^0-9.]/g, ''); this.value = this.value.replace(/(\..*)\./g, '$1');" class="form-control" id="UnitPrice" name="UnitPrice" data-ng-model="objPurchaseOrderDetail.UnitPrice" placeholder="UnitPrice" maxlength="5" required>
                                                    @*<span class="errorMsg" ng-show="PODetsubmitted && FormPurchaseOrderDetailForm.UnitPrice.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;UnitPrice is required.</span>*@
                                                    @*<span class="errorMsg" ng-show="PODetsubmitted && FormPurchaseOrderDetailForm.UnitPrice.$error.number"><i class="fa fa-exclamation-triangle"></i> &nbsp;Enter Proper Numnber.</span>*@
