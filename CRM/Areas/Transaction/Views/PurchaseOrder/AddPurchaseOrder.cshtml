                                                    </elaunch-autocomplete>
                                                    <span class="errorMsg" ng-show="submitted && objPurchaseOrder.BuyerData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;BuyerData is required.</span>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label for="SupplierId" class="control-label">Company Name<span class="text-danger">*</span></label>
                                                    <elaunch-autocomplete min-chars="0" mode="BuyerMaster" id="Supplier" isdisplay="true"
                                                                          placeholder="Enter Supplier" tabindex="4"
                                                                          value-changed="callback(value)" ng-model="objPurchaseOrder.SupplierData">
                                                    </elaunch-autocomplete>
                                                    <span class="errorMsg" ng-show="submitted && (objPurchaseOrder.SupplierData.Display == '' || objPurchaseOrder.SupplierData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Supplier is required.</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label for="BuyerAddress" class="control-label">Address@*<span class="text-danger">*</span>*@ </label>
