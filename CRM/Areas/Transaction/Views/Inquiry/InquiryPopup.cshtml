<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">Manage Inquiry Product Detail</h3>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="box">
                    <div class="box-body no-padding">
                        <form accessible-form role="form" name="FormInquiryItemDetail" id="FormInquiryItemDetail" ng-class="{'submitted': submitted}" ng-submit="submitted = true" novalidate>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="CategoryId" class="control-label">Product Main Category :<b class="errorMsg">*</b></label>
                                            <elaunch-autocomplete min-chars="0" mode="CategoryMaster" id="Category" isdisplay="true"
                                                                  placeholder="Enter Category"
                                                                  value-changed="callback(value)" ng-model="objInquiryItemDetails.CategoryData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && objInquiryItemDetails.CategoryData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Product Main Category is required.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="SubCategoryId" class="control-label">Product Sub Category :<b class="errorMsg">*</b></label>
                                            <elaunch-autocomplete min-chars="0" mode="SubCategoryMaster" id="subcategory" isdisplay="objInquiryItemDetails.CategoryData.Display != ''"
                                                                  placeholder="Enter Sub Category" related-value="objInquiryItemDetails.CategoryData.Value"
                                                                  value-changed="callback(value)" ng-model="objInquiryItemDetails.SubCategoryData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && objInquiryItemDetails.SubCategoryData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Product Sub Category is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    @*<div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="MainProductId" class="control-label">Main Product :<b class="errorMsg">*</b></label>
                                            <elaunch-autocomplete min-chars="0" mode="MainProductMaster" id="MainProduct" isdisplay="objInquiryItemDetails.SubCategoryData.Display != ''"
                                                                  placeholder="Enter Main Product" related-value="objInquiryItemDetails.SubCategoryData.Value"
                                                                  value-changed="callback(value)" ng-model="objInquiryItemDetails.MainProductData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && objInquiryItemDetails.MainProductData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Main Product is required.</span>
                                        </div>
                                    </div>*@
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="ProductId" class="control-label">Product :<b class="errorMsg">*</b></label>
                                            <elaunch-autocomplete min-chars="0" mode="ProductMaster" id="Product" isdisplay="objInquiryItemDetails.SubCategoryData.Display != ''"
                                                                  placeholder="Enter Product" related-value="objInquiryItemDetails.SubCategoryData.Value"
                                                                  value-changed="callback(value)" ng-model="objInquiryItemDetails.ProductData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && objInquiryItemDetails.ProductData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Product is required.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="ProductDescription" class="control-label">Product Description : <b class="errorMsg">*</b> </label>
                                            <textarea class="form-control" id="ProductDescription" name="ProductDescription" data-ng-model="objInquiryItemDetails.ProductDescription" placeholder="Product Description" rows="3" cols="60" style="max-width: 100%;" tabindex="95" required></textarea>
                                            <span class="errorMsg" ng-show="submitted && FormInquiryItemDetail.ProductDescription.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Product Description is required.</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group">
                                            <label for="QtyCode">Qty Code<b class="errorMsg">*</b></label>
                                            <elaunch-autocomplete min-chars="0" mode="UnitMaster" id="Unit" isdisplay="true"
                                                                  placeholder="Enter Qty Code"
                                                                  value-changed="callback(value)" ng-model="objInquiryItemDetails.QtyCodeValueData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && objInquiryItemDetails.QtyCodeValueData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Qty Code is required.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-8">
                                        <div class="form-group">
                                            <label for="Qty">Qty<b class="errorMsg">*</b> </label>
                                            <input type="text" class="form-control" id="Qty" name="Qty" data-ng-model="objInquiryItemDetails.Qty" placeholder="Qty" maxlength="20" required tabindex="96" ng-pattern="/^\d+([,.]\d+)?$/">
                                            <span class="errorMsg" ng-show="submitted && FormInquiryItemDetail.Qty.$error.required "><i class="fa fa-exclamation-triangle"></i> &nbsp;Qty is required.</span>
                                            <span class="errorMsg" ng-show="submitted && FormInquiryItemDetail.Qty.$error.pattern "><i class="fa fa-exclamation-triangle"></i> &nbsp;Enter numeric values.</span>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-click="objInquiryItemDetails.CategoryData.Display != '' && objInquiryItemDetails.UnitPriceCodeValueData.Display != '' && objInquiryItemDetails.QtyCodeValueData.Display != '' && objInquiryItemDetails.ProductData.Display != '' && objInquiryItemDetails.OriginData.Display != '' && objInquiryItemDetails.SubCategoryData.Display != '' && FormInquiryItemDetail.$valid && CreateUpdate(objInquiryItemDetails)" tabindex="101"> <i class="fa fa-save"></i>Submit</button>
                                <button type="button" class="btn btn-default" data-ng-click="close()" tabindex="102">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
