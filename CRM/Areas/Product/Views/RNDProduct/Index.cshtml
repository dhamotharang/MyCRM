
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div ng-controller="RndProductCtrl" ng-init="myinit()">
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN PORTLET-->
            <div class="portlet light bordered" id="Griddata">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <elaunchgrid data-grid-obj="gridObj" mode="RNDProductMaster" title="R & D Product List" set-fn="setDirectiveRefresh(theDirFn)" onaddnew="Add()"></elaunchgrid>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END PORTLET-->
        </div>
    </div>
</div>


@*<script type="text/ng-template" id="ProductModalContent.html">
        <ng-include src="'@Url.Action("AddProduct", "Product")'"></ng-include>
    </script>*@

<script type="text/javascript">
    CKEDITOR_BASEPATH = "@Url.Content("~/Scripts/ckeditor/")";
</script>


@section Scripts
{
    @Scripts.Render("~/bundles/CkEditor")
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/RNDProduct")
}
