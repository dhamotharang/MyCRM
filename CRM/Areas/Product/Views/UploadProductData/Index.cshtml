
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div ng-controller="UploadProductDataCtrl">
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN PORTLET-->
            <div class="portlet light bordered" id="Griddata">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <elaunchgrid data-grid-obj="gridObj" mode="UploadProductDataMaster" title="Upload Product Data List" set-fn="setDirectiveRefresh(theDirFn)" onaddnew="Add(0)" is-add="false"></elaunchgrid>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END PORTLET-->
        </div>
    </div>
</div>
<script type="text/ng-template" id="ProductFormShare.html">
    <ng-include src="'@Url.Action("ProductFormShare", "ProductForm")'"></ng-include>
</script>
@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/UploadProductData")
}
