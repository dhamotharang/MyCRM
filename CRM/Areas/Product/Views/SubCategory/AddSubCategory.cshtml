<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">SubCategory Information</h3>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="box">
                    <div class="box-body no-padding">
                        <form role="form" accessible-form name="FormCategoryInfo" id="FormCategoryInfo" ng-class="{'submitted': submitted}" ng-submit="submitted=true" novalidate>
                            <fieldset ng-disabled="isClicked">
                                <div class="modal-body">
                                    <div class="form-group">
                                        <label>Main Category Name:<span class="text-danger">*</span> </label>
                                        @*<elaunch-autocomplete min-chars="0" mode="CategoryMaster" id="category" isdisplay="true"
                                                                  placeholder="Enter Category" name="category"
                                                                  value-changed="callback(value)" ng-model="objSubCategory.CategoryData.Value" ng-required="true">
                                            </elaunch-autocomplete>*@

                                        <elaunch-autocomplete min-chars="0" mode="CategoryMaster" isdisplay="true"
                                                              placeholder="Enter Category" id="Category"
                                                              value-changed="callback(value)" ng-model="objSubCategory.CategoryData" ng-required="true">
                                        </elaunch-autocomplete>
                                        <span class="errorMsg" ng-show="submitted && (objSubCategory.CategoryData.Display == '' || objSubCategory.CategoryData.Display == null)"><i class="fa fa-exclamation-triangle"></i>&nbsp;Category Name is required.</span>
                                    </div>
                                    <div class="form-group">
                                        <label>Sub Category Name:<span class="text-danger">*</span> </label>
                                        <input type="text" camel-text maxlength="100" class="form-control" id="SubCategoryName" name="SubCategoryName" data-ng-model="objSubCategory.SubCategoryName" placeholder="Sub Category Name" required>
                                        <span class="errorMsg" ng-show="submitted && FormCategoryInfo.SubCategoryName.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Sub Category Name is required.</span>
                                    </div>
                                </div>
                            </fieldset>
                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="objSubCategory.SubCategoryId <= 0 && isClicked == false" data-ng-click="objSubCategory.CategoryData.Display != '' && FormCategoryInfo.$valid && CreateUpdate(objSubCategory)" > <i class="fa fa-save"></i>Save</button>
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="objSubCategory.SubCategoryId > 0 && isClicked == false" data-ng-click="objSubCategory.CategoryData.Display != '' && FormCategoryInfo.$valid && CreateUpdate(objSubCategory)" ><i class="fa fa-edit"></i>Update</button>
                                @*    <button type="button" class="btn btn-default btn-success" data-ng-click="Reset()" tabindex="15">Reset</button>*@
                                <button type="button" class="btn btn-default" data-ng-click="close()">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

