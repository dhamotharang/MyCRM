@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div data-ng-controller="HolidayNameCtrl">
    <div class="row">
        <div class="col-md-12">
            <div class="portlet light bordered">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <elaunchgrid data-grid-obj="gridObj" mode="HolidayNameMaster" title="Holiday Name List" set-fn="setDirectiveRefresh(theDirFn)" onaddnew="Add(objHolidayName)"></elaunchgrid>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/ng-template" id="HolidayNameModal.html">
    <ng-include src="'@Url.Action("AddHolidayName", "HolidayName")'"></ng-include>
</script>

@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/HolidayName")
}

