<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">Account Type Information</h3>
        </div>
    </div>
    <div class="col-sm-12">
        <div class="box">
            <div class="box-body no-padding">
                <form accessible-form role="form" name="FormAccountTypeInfo" id="FormAccountTypeInfo" tabindex="21" ng-class="{'submitted': submitted}" ng-submit="submitted=true" novalidate>
                    <fieldset ng-disabled="isClicked">
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label for="AccountType">Account Type:<span class="text-danger">*</span> </label>
                                        <input type="text" class="form-control" id="AccountType" name="AccountType" data-ng-model="objAccountType.AccountType" placeholder="Account Type" required maxlength="50">
                                        @*<div data-ng-show="FormAccountTypeInfo.AccountType.$dirty && FormAccountTypeInfo.AccountType.$invalid " class="label label-danger">
                                                invalid:<span data-ng-show="FormAccountTypeInfo.AccountType.$error.required ">Tell us Account Type.</span>
                                            </div>*@
                                        <span class="errorMsg" ng-show="submitted && FormAccountTypeInfo.AccountType.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Account Type is required.</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="objAccountType.AccountTypeId <= 0 && isClicked == false" " data-ng-click=" FormAccountTypeInfo.$valid && CreateUpdate(objAccountType)"> <i class="fa fa-save"></i>Save</button>
                        <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="objAccountType.AccountTypeId > 0 && isClicked == false" " data-ng-click=" FormAccountTypeInfo.$valid && CreateUpdate(objAccountType)"><i class="fa fa-edit"></i>Update</button>
                        @*<button type="button" class="btn btn-default btn-success" data-ng-click="Reset()" tabindex="25">Reset</button>*@
                        <button type="button" class="btn btn-default" data-ng-click="close()">Close</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
