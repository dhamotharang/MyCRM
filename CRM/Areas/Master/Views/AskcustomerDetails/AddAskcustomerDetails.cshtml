<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">Manage Ask customer Information</h3>
        </div>
        <div class="row"  ng-init="setvalue()">
            <div class="col-sm-12">
                <div class="box">
                    <div class="box-body no-padding">
                        <form accessible-form role="form" name="FormAskCustomerInfo" id="FormAskCustomerInfo" ng-class="{'submitted': submitted}" ng-submit="submitted=true" novalidate>
                            <fieldset ng-disabled="isClicked">
                                <div class="modal-body">
                                    <div class="form-group">
                                        <label for="Date" class="control-label">Inquiry Date:<span class="text-danger">*</span></label>
                                        <div class="form-group">
                                            <elaunch-datepicker ng-model="objAskCustomer.Date" name="Date" date-options="dateOptions" week-off="0" tabindex="3"></elaunch-datepicker>
                                            <span class="errorMsg" ng-show="submitted && (objAskCustomer.Date == '' || objAskCustomer.Date == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Date is required.</span>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="SourceData" class="control-label">Source: <span class="text-danger">*</span></label>
                                        <elaunch-autocomplete min-chars="0" mode="SourceMaster" id="Source" isdisplay="true"
                                                              placeholder="Enter Source"
                                                              value-changed="callback(value)" ng-model="objAskCustomer.SourceData">
                                        </elaunch-autocomplete>
                                        <span class="errorMsg" ng-show="submitted && (objAskCustomer.SourceData.Display == '' || objAskCustomer.SourceData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Source Name is required.</span>
                                    </div>
                                    <div class="form-group">
                                        <label for="Name" class="control-label">Buyer Name/Remark : </label>
                                        <input type="text" id="Name" name="Name"  class="form-control" data-ng-model="objAskCustomer.Name" placeholder="Buyer Name" tabindex="6"/>
                                        @*<textarea class="form-control addressbox" id="Name" name="Name" data-ng-model="objAskCustomer.Name" placeholder="Name" rows="2" cols="60" style="max-width: 100%;" tabindex="6"></textarea>*@
                                    </div>
                                    <div class="form-group">
                                        <label for="Requirement" class="control-label">Requirement: </label>
                                        <textarea class="form-control addressbox" id="Requirement" name="Requirement" data-ng-model="objAskCustomer.Requirement" placeholder="Requirement" rows="2" cols="60" style="max-width: 100%;" tabindex="6"></textarea>
                                    </div>
                                    <div class="form-group">
                                        <label for="MobCode" class="control-label">Mobile No.:</label>
                                        <elaunch-mobile ng-model="mobile" placeholder="Enter Mobile Number" telcode-array="MobCodeData" tabindex="7" ng-required="false"></elaunch-mobile>
                                        @*<span class="errorMsg" ng-show="submitted && (FormAskCustomerInfo.mobile.$error.required || mobile == null || mobile == '') "><i class="fa fa-exclamation-triangle"></i> &nbsp;Mobile is required.</span>*@
                                    </div>
                                    <div class="form-group">
                                        <label for="Email" class="control-label">Email : </label>
                                        <elaunch-Email ng-model="emailid" telcode-array="emailCodeData" tabindex="8" ng-required="false" ></elaunch-Email>
                                        @*<span class="errorMsg" ng-show="submitted && (FormAskCustomerInfo.emailid.$error.required || emailid == null || emailid == '')"><i class="fa fa-exclamation-triangle"></i> &nbsp;Email is required.</span>*@
                                    </div>
                                </div>
                            </fieldset>
                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="objAskCustomer.AskCustId <= 0 && isClicked == false" data-ng-click="objAskCustomer.SourceData.Display!='' && CreateUpdate(objAskCustomer)"> <i class="fa fa-save"></i>Save</button>
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="objAskCustomer.AskCustId > 0 && isClicked == false" data-ng-click="objAskCustomer.SourceData.Display!='' && CreateUpdate(objAskCustomer)"><i class="fa fa-edit"></i>Update</button>
                                <button type="button" class="btn btn-default" data-ng-click="close()">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
