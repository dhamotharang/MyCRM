<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">Manage Specification Information</h3>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="box">
                    <div class="box-body no-padding">
                        <form accessible-form role="form" name="FormSpecificationInfo" id="FormSpecificationInfo" ng-class="{'submitted': submitted}" ng-submit="submitted = true" novalidate>
                            <fieldset ng-disabled="isClicked">
                                <div class="modal-body">
                                    <div class="form-group">
                                        <label for="TechHeaddata" class="control-label">Technical Specification Head: <span class="text-danger">*</span></label>
                                        <elaunch-autocomplete min-chars="0" mode="TechnicalSpecHeadMaster" id="TechHead" isdisplay="true"
                                                              placeholder="Enter address"
                                                              value-changed="callback(value)" ng-model="objSpecification.TechHeaddata">
                                        </elaunch-autocomplete>
                                        <span class="errorMsg" ng-show="submitted && (objSpecification.TechHeaddata.Display == '' || objSpecification.TechHeaddata.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Please select Technical Specification Head.</span>
                                    </div>
                                    <div class="form-group">
                                        <label for="TechSpec">Specification :<span class="text-danger">*</span> </label>
                                        <input type="text" class="form-control" id="TechSpec" name="TechSpec" data-ng-model="objSpecification.TechSpec" placeholder="Specification" required maxlength="100">
                                        <span class="errorMsg" ng-show="submitted && FormSpecificationInfo.TechSpec.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Specification is required.</span>
                                    </div>
                                </div>
                            </fieldset>
                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="id <= 0 && isClicked == false" data-ng-click="FormSpecificationInfo.$valid && objSpecification.TechHeaddata.Display != '' && CreateUpdate(objSpecification)"> <i class="fa fa-save"></i>Save</button>
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="id > 0 && isClicked == false" data-ng-click="FormSpecificationInfo.$valid && objSpecification.TechHeaddata.Display != '' && CreateUpdate(objSpecification)"><i class="fa fa-edit"></i>Update</button>
                                @*     <button type="button" class="btn btn-default btn-success" data-ng-click="Reset()" tabindex="4">Reset</button>*@
                                <button type="button" class="btn btn-default" data-ng-click="close()">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
