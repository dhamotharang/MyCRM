
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div ng-controller="DailyReportingCtrl" ng-init="myinit()">
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN PORTLET-->
            <div class="portlet light bordered">

                <div class="portlet-body">
                    <div>
                        <div>
                            @*<input type="button" class="btn btn-success" ng-click="AddCurrency();" value="Add Currency" data-toggle="modal" data-target="#myModal" />*@

                            <div class="row">
                                <div class="col-sm-12">
                                    <elaunchgrid data-grid-obj="gridObj" mode="DailyReportingMaster" onaddnew="Add(0)" set-fn="setDirectiveRefresh(theDirFn)" title="Daily Work Report List"></elaunchgrid>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <!-- END PORTLET-->
        </div>
    </div>
</div>

<script type="text/ng-template" id="myModalContent.html">
    <ng-include src="'@Url.Action("DailyReport", "DailyReporting")'"></ng-include>
</script>

@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/DailyReporting")
}




