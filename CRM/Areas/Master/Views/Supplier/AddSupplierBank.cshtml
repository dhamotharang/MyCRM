
<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">Manage Supplier Bank Detail</h3>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="box">
                    <div class="box-body no-padding">
                        <form accessible-form role="form" name="FormSupplierBankDetail" id="FormSupplierBankDetail" ng-submit="submitted=true" ng-class="{'submitted': submitted}" novalidate>
                            <div class="modal-body">

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="BeneficiaryName">Beneficiary Name:<span class="text-danger">*</span> </label>
                                            <input type="text" class="form-control" id="BeneficiaryName" name="BeneficiaryName" data-ng-model="objSupplierBank.BeneficiaryName" placeholder="Beneficiary Name" maxlength="100" required>
                                            <span class="errorMsg" ng-show="submitted && FormSupplierBankDetail.BeneficiaryName.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Beneficiary Name is required.</span>
                                        </div>
                                    </div>
                                </div>

                                @*<div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="NickName">Nick Name: </label>
                                            <input type="text" class="form-control" id="NickName" name="NickName" data-ng-model="objSupplierBank.NickName" placeholder="Nick Name" maxlength="100">
                                        </div>
                                    </div>
                                </div>*@

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="BankName">Bank Name:<span class="text-danger">*</span> </label>
                                            <elaunch-autocomplete min-chars="0" mode="BankNameMaster" id="Bank" isdisplay="true"
                                                                  placeholder="Enter Bank"
                                                                  value-changed="callback(value)" ng-model="objSupplierBank.BankData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && (objSupplierBank.BankData.Display =='' || objSupplierBank.BankData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Bank Name is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="BranchName">Branch Name:<span class="text-danger">*</span> </label>
                                            <input type="text" class="form-control" id="BranchName" name="BranchName" data-ng-model="objSupplierBank.BranchName" placeholder="Branch Name" maxlength="100" required>
                                            <span class="errorMsg" ng-show="submitted && FormSupplierBankDetail.BranchName.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Branch Name is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="AccountType">Account Type:<span class="text-danger">*</span> </label>
                                            <elaunch-autocomplete min-chars="0" mode="AccountTypeMaster" id="AccountType" isdisplay="true"
                                                                  placeholder="Select Account Type"
                                                                  value-changed="callback(value)" ng-model="objSupplierBank.AccountTypeData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && (objSupplierBank.AccountTypeData.Display =='' || objSupplierBank.AccountTypeData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Account Type is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="AccountNo">Account No:<span class="text-danger">*</span> </label>
                                            <input type="text" oninput="this.value = this.value.replace(/[^0-9.]/g, ''); this.value = this.value.replace(/(\..*)\./g, '$1');" class="form-control" id="AccountNo" name="AccountNo" data-ng-model="objSupplierBank.AccountNo" placeholder="Account No." maxlength="100" required>
                                            <span class="errorMsg" ng-show="submitted && FormSupplierBankDetail.AccountNo.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Account No is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="IFSCCode">IFSC Code:@*<span class="text-danger">*</span>*@ </label>
                                            <input type="text" class="form-control" id="IFSCCode" name="IFSCCode" data-ng-model="objSupplierBank.IFSCCode" placeholder="IFSC Code" maxlength="50" required>
                                            <span class="errorMsg" ng-show="submitted && FormSupplierBankDetail.IFSCCode.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;IFSC Code is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="SwiftCode">Swift Code:@*<span class="text-danger">*</span>*@ </label>
                                            <input type="text" class="form-control" id="SwiftCode" name="SwiftCode" data-ng-model="objSupplierBank.SwiftCode" placeholder="Swift Code" maxlength="50">
                                            @*<span class="errorMsg" ng-show="submitted && FormSupplierBankDetail.SwiftCode.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Swift Code is required.</span>*@
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-click="FormSupplierBankDetail.$valid && CreateUpdate(objSupplierBank)"> <i class="fa fa-save"></i>Submit</button>
                                <button type="button" class="btn btn-default" data-ng-click="close()">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


