
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div ng-controller="ReceiptVoucherController">

    <ul class="page-breadcrumb breadcrumb">
        <li>
            <a href="#">Home</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span class="active">ReceiptVoucher</span>
        </li>
    </ul>

    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN PORTLET-->
            <div class="portlet light bordered">

                <div class="portlet-body">
                    <div>
                        <div>

                            <div class="row">
                                <div class="col-sm-12">
                                    <elaunchgrid data-grid-obj="gridObj" mode="ReceiptVoucherMaster" set-fn="setDirectiveRefresh(theDirFn)" onaddnew="Add(0)" title="Receipt Voucher List"></elaunchgrid>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END PORTLET-->
        </div>
    </div>
    <div id="myConfirmModal" class="modal fade" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
            </div>
        </div>
    </div>
</div>
<script type="text/ng-template" id="myModalContent.html">
    <ng-include src="'@Url.Action("ReceiptVoucherPopup", "ReceiptVoucher")'"></ng-include>
</script>
@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/ReceiptVoucher")
}
