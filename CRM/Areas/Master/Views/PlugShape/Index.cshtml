
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div data-ng-controller="PlugShapeCtrl">

    <div class="row">
        <div class="col-md-12">
            <div class="portlet light bordered">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <elaunchgrid data-grid-obj="gridObj" mode="PlugShapeMaster" title="Plug Shape List" set-fn="setDirectiveRefresh(theDirFn)" onaddnew="Add(objPlugShape)"></elaunchgrid>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/ng-template" id="PlugShapeModal.html">
    <ng-include src="'@Url.Action("AddPlugShape", "PlugShape")'"></ng-include>
</script>


@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/PlugShape")
}




