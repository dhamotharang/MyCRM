@{
    ViewBag.Title = "AddUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string TempImg = System.Configuration.ConfigurationManager.AppSettings["TempImgPath"];
    string UserImagePath = System.Configuration.ConfigurationManager.AppSettings["UserImagePath"];
}

<!DOCTYPE html>
<style>
    .isMultiwidth .multiSelect .buttonLabel {
        width: 125px !important;
    }

    ul.tab {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
        border: 1px solid #ccc;
        background-color: #f1f1f1;
    }

        /* Float the list items side by side */
        ul.tab li {
            float: left;
        }

            /* Style the links inside the list items */
            ul.tab li a {
                display: inline-block;
                color: black;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
                transition: 0.3s;
                font-size: 12px;
                font-weight: bold;
            }

                /* Change background color of links on hover */
                ul.tab li a:hover {
                    background-color: #ddd;
                }

                /* Create an active/current tablink class */
                ul.tab li a:focus, .active {
                    background-color: #ccc;
                }

    /* Style the tab content */
    .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #ccc;
        border-top: none;
    }
</style>
<div data-ng-controller="UserController">
    <div class="row" data-ng-init="SetUserId(@ViewBag.id,@ViewBag.usertype,@ViewBag.isdisable)">
        <div class="col-sm-12">
            <div class="row">
                <div class="col-sm-12">
                    <div class="box">
                        <div class="box-body no-padding">
                            <form accessible-form role="form" name="FormUserInfo" id="FormUserInfo" ng-class="{'submitted': submitted}" ng-submit="submitted=true" novalidate>
                                <fieldset ng-disabled="isClicked">
                                    <div class="box box-info">
                                        <div class="box-header with-border">
                                            <h3 class="box-title">
                                                User Information
                                            </h3>
                                        </div>
                                    </div>
                                    <ul class="tab">
                                        <li><a ng-class="{active : tabname == 'EmployeeReference'}" data-ng-click="openTab(event, 'EmployeeReference',objUser)">Source</a></li>
                                        <li><a ng-class="{active : tabname == 'JoiningDetails'}" data-ng-click="openTab(event, 'JoiningDetails',objUser)">Joining</a></li>
                                        <li><a ng-class="{active : tabname == 'AllotmentDetails'}" data-ng-click="openTab(event, 'AllotmentDetails',objUser)">Allotment</a></li>
                                        <li><a ng-class="{active : tabname == 'SalaryDetails'}" data-ng-click="openTab(event, 'SalaryDetails',objUser)">Salary</a></li>
                                        <li><a ng-class="{active : tabname == 'BasicDetails'}" data-ng-click="openTab(event, 'BasicDetails',objUser)">Basic</a></li>
                                        <li><a ng-class="{active : tabname == 'ContactDetails'}" data-ng-click="openTab(event, 'ContactDetails',objUser)">Contact</a></li>
                                        <li><a ng-class="{active : tabname == 'AdressDetails'}" data-ng-click="openTab(event, 'AdressDetails',objUser)">Address</a></li>
                                        <li><a ng-class="{active : tabname == 'BirthDetails'}" data-ng-click="openTab(event, 'BirthDetails',objUser)">Birth</a></li>
                                        <li><a ng-class="{active : tabname == 'HomeTownDetails'}" data-ng-click="openTab(event, 'HomeTownDetails',objUser)">Home Town</a></li>
                                        <li><a ng-class="{active : tabname == 'WorkEducationDetails'}" data-ng-click="openTab(event, 'WorkEducationDetails',objUser)">Education</a></li>
                                        <li><a ng-class="{active : tabname == 'WorkExperienceDetails'}" data-ng-click="openTab(event, 'WorkExperienceDetails',objUser)">Experience</a></li> @*New Tab Add*@
                                        <li><a ng-class="{active : tabname == 'FamilyMemberDetails'}" data-ng-click="openTab(event, 'FamilyMemberDetails',objUser)">Family</a></li>
                                        <li><a ng-class="{active : tabname == 'ReferenceDetails'}" data-ng-click="openTab(event, 'ReferenceDetails',objUser)">Reference</a></li>
                                        <li><a ng-class="{active : tabname == 'DocumentsDetails'}" data-ng-click="openTab(event, 'DocumentsDetails',objUser)">Documents</a></li>
                                        <li><a ng-class="{active : tabname == 'BankDetails'}" data-ng-click="openTab(event, 'BankDetails',objUser)">Bank</a></li>

                                    </ul>
                                    <div class="box-body">
                                        <div id="EmployeeReference" class="panel panel-primary tabcontent">
                                            <div class="panel-body">
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label>Sources :@*<span class="text-danger">*</span>*@</label>
                                                        <elaunch-autocomplete min-chars="0" mode="SourceMaster" id="SourceId" isdisplay="true"
                                                                              placeholder="Enter Sources" ng-required="false"
                                                                              value-changed="callback(value)" ng-model="objUser.SourceData">
                                                        </elaunch-autocomplete>
                                                        <span class="errorMsg" ng-show="submitted && (objUser.SourceData.Display == ''  && objUser.SourceData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Source is required.</span>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label>Referance Type :</label>@*<span class="text-danger">*</span>*@<br />
                                                            <input type="radio" id="ReferenceTypeId" value="1" ng-change="showHide('BuyerMaster')" name="ReferenceTypeId" data-ng-model="objUser.ReferenceTypeId">&nbsp;Contact&nbsp;&nbsp;
                                                            <input type="radio" id="ReferenceTypeId" value="2" ng-change="showHide('UserMaster')" name="ReferenceTypeId" data-ng-model="objUser.ReferenceTypeId">&nbsp;Employee&nbsp;&nbsp;
                                                            <input type="radio" id="ReferenceTypeId" value="3" ng-change="showHide('Other')" name="ReferenceTypeId" data-ng-model="objUser.ReferenceTypeId">&nbsp;Other&nbsp;&nbsp;
                                                            @*<span class="errorMsg" ng-show="submitted && (objUser.ReferenceTypeId == 0)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Reference Type is required.</span>*@
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group" ng-show="showHideReferance" ng-model="showHideReferance">
                                                            <label>Referance Name :@*<span class="text-danger">*</span>*@</label>
                                                            <elaunch-autocomplete min-chars="0" mode="{{ReferanceMode}}" id="ReferenceId" isdisplay="true"
                                                                                  placeholder="Enter Referance Name" ng-required="false"
                                                                                  value-changed="callback(value)" ng-model="objUser.ReferanceData" tabindex="74">
                                                            </elaunch-autocomplete>
                                                            @*<span class="errorMsg" ng-show="submitted && (showHideReferance && objUser.ReferanceData.Display == '')"><i class="fa fa-exclamation-triangle"></i> &nbsp;Referance Name is required.</span>*@
                                                        </div>
                                                        <div class="form-group" ng-show="showHideAgency" ng-model="showHideAgency">
                                                            <label>Reference Name :@*<span class="text-danger">*</span>*@</label>
                                                            <input type="text" class="form-control" id="ReferenceMannualEntry" required maxlength="200" name="ReferenceMannualEntry" data-ng-model="objUser.ReferenceMannualEntry" ng-bind="objUser" placeholder="Reference Name" tabindex="75">
                                                            @*<span class="errorMsg" ng-show="submitted && (showHideAgency && FormUserInfo.ReferenceMannualEntry.$error.required)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Reference Name is required.</span>*@
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            @*<a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'BankDetails')">Back</a>*@
                                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'JoiningDetails',objUser)">Save & Continue</a>
                                        </div>

                                        <div id="JoiningDetails" class="panel panel-primary tabcontent">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group" ng-class="{ 'has-error' :FormUserInfo.CommunicationDate.$invalid && !FormUserInfo.CommunicationDate.$pristine }">
                                                            <label>Date of first Communication :<span class="text-danger">*</span> </label>
                                                            <elaunch-datepicker ng-model="objUser.CommunicationDate" date-options="dateOptions" is-required="true" ng-required="false" tabindex="64"></elaunch-datepicker>
                                                        </div>
                                                    </div>

                                                    <div class="col-sm-6">
                                                        <div class="form-group" ng-class="{ 'has-error' :FormUserInfo.JoiningDate.$invalid && !FormUserInfo.JoiningDate.$pristine }">
                                                            <label>Joining Date :<span class="text-danger">*</span> </label>
                                                            <elaunch-datepicker ng-model="objUser.JoiningDate" date-options="dateOptions" is-required="true" ng-required="false" tabindex="64"></elaunch-datepicker>
                                                        </div>
                                                    </div>


                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="DesignationId">Designation: <span class="text-danger">*</span> </label>
                                                            <elaunch-autocomplete min-chars="0" mode="DesignationMaster" id="DesignationId" isdisplay="true"
                                                                                  placeholder="Enter Designation" ng-required="false"
                                                                                  value-changed="callback(value)" ng-model="objUser.DesignationData" tabindex="66">
                                                            </elaunch-autocomplete>
                                                        </div>

                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="DepartmentId">Department:<span class="text-danger">*</span> </label>
                                                            <elaunch-autocomplete min-chars="0" mode="DepartmentMaster" id="DepartmentId" isdisplay="true"
                                                                                  placeholder="Enter Department" ng-required="false"
                                                                                  value-changed="callback(value)" ng-model="objUser.DepartmentData" tabindex="65">
                                                            </elaunch-autocomplete>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="ReportingId">Reporting To: </label>
                                                            <elaunch-autocomplete min-chars="0" mode="UserMaster" id="ReportingId" isdisplay="true"
                                                                                  placeholder="Enter Reporting Person" ng-required="false"
                                                                                  value-changed="callback(value)" ng-model="objUser.ReportingData" tabindex="68">
                                                            </elaunch-autocomplete>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="RoleId">Role & Rights : <span class="text-danger">*</span> </label>
                                                            <elaunch-autocomplete min-chars="0" mode="RoleMaster" id="RoleId" isdisplay="true"
                                                                                  placeholder="Enter Role" ng-required="false"
                                                                                  value-changed="callback(value)" ng-model="objUser.RoleData" tabindex="67">
                                                            </elaunch-autocomplete>
                                                        </div>
                                                    </div>

                                                </div>

                                                <div class="panel panel-primary">
                                                    <div class="panel-heading">
                                                        <h3 class="panel-title" style="margin:0">Working Time/Shift Details</h3>
                                                    </div>
                                                    <div class="panel-body">
                                                        <div class="row">
                                                            <div>
                                                                <div class="col-sm-3">
                                                                    <div class="form-group">
                                                                        <label for="ShiftStartTime">Shift Start :<span class="text-danger">*</span></label>
                                                                        <elaunch-timepicker ng-model="objUser.ShiftStartTime" hstep="1" mstep="1" is-meridian="ismeridian" is-required="false" tabindex="69"></elaunch-timepicker>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-3">
                                                                    <div class="form-group">
                                                                        <label for="ShiftEndTime">Shift End :<span class="text-danger">*</span></label>
                                                                        <elaunch-timepicker ng-model="objUser.ShiftEndTime" hstep="1" mstep="1" is-meridian="ismeridian" is-required="false" tabindex="69"></elaunch-timepicker>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                            <div>
                                                                <div class="col-sm-3">
                                                                    <div class="form-group">
                                                                        <label for="LunchStartTime">Lunch Start :<span class="text-danger">*</span></label>
                                                                        <elaunch-timepicker ng-model="objUser.LunchStartTime" hstep="1" mstep="1" is-meridian="ismeridian" is-required="false" tabindex="70"></elaunch-timepicker>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-3">
                                                                    <div class="form-group">
                                                                        <label for="LunchEndTime">Lunch End :<span class="text-danger">*</span></label>
                                                                        <elaunch-timepicker ng-model="objUser.LunchEndTime" hstep="1" mstep="1" is-meridian="ismeridian" is-required="false" tabindex="70"></elaunch-timepicker>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                <div class="form-group">
                                                                    <label for="ShiftHours">Shift Hours :</label>
                                                                    <br />
                                                                    <b>{{objUser.hours}}</b>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-6">
                                                                <div class="form-group">
                                                                    <label for="LunchHours">Lunch Hours :</label>
                                                                    <br />
                                                                    <b>{{objUser.Lunchhours}}</b>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'EmployeeReference')">Back</a>
                                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'AllotmentDetails',objUser)">Save & Continue</a>
                                        </div>

                                        <div id="AllotmentDetails" class="panel panel-primary tabcontent">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="MobCode">Mobile No.:</label>@*<span class="text-danger">*</span>*@
                                                            <elaunch-mobile ng-model="MobCode" placeholder="Enter Mobile Number" telcode-array="MobCodeData" ng-required="false"></elaunch-mobile>
                                                            @*<span class="errorMsg" ng-show="Allotmentsubmitted && (MobCode == null || MobCode == '') "><i class="fa fa-exclamation-triangle"></i> &nbsp;Mobile No is required.</span>*@
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="Email">Email : @*<span class="text-danger">*</span>*@</label>
                                                            <elaunch-Email ng-model="Email" telcode-array="emailCodeData" ng-required="false"></elaunch-Email>
                                                            @*<span class="errorMsg" ng-show="Allotmentsubmitted && (Email == null || Email == '')"><i class="fa fa-exclamation-triangle"></i> &nbsp;Email ID is required.</span>*@
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="Chat">Chat : @*<span class="text-danger">*</span>*@</label>
                                                            <elaunch-chat ng-model="chat" chatcode-array="chatCodeData" ng-required="false"></elaunch-chat>
                                                            @*<span class="errorMsg" ng-show="Allotmentsubmitted && (chat == null || chat == '') "><i class="fa fa-exclamation-triangle"></i> &nbsp;Chat is required.</span>*@
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'JoiningDetails')">Back</a>
                                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'SalaryDetails',objUser)">Save & Continue</a>
                                        </div>

                                        <div id="SalaryDetails" class="panel panel-primary tabcontent">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="CategoryName">Salary Head<span class="text-danger">*</span> </label>
                                                            <elaunch-autocomplete min-chars="0" mode="SalaryHeadMaster" isdisplay="true"
                                                                                  placeholder="Enter Salary" id="Salary"
                                                                                  value-changed="callback(value)" name="Salary" ng-model="objUserSalary.SalaryHeadData" ng-required="false">
                                                            </elaunch-autocomplete>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="CurrencyName">Currency Name:<span class="text-danger">*</span> </label>
                                                            <elaunch-autocomplete min-chars="0" mode="CurrencyMaster" id="CurrencyName" isdisplay="true"
                                                                                  placeholder="Enter CurrencyName" name="CurrencyName"
                                                                                  value-changed="callback(value)" ng-model="objUserSalary.CurrencyData" ng-required="false">
                                                            </elaunch-autocomplete>
                                                            @*<span class="errorMsg" ng-show="submitted && (objUserSalary.CurrencyData.Display == '' || objUserSalary.CurrencyData.Display == null) "><i class="fa fa-exclamation-triangle"></i> &nbsp;CurrencyName is required.</span>*@
                                                        </div>
                                                    </div>
                                                </div>


                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group" data-ng-if="objUserSalary.CurrencyData.Value != 1">
                                                            <label for="ExchangeRate">Exchange Rate:</label>
                                                            <input type="text" class="form-control" id="ExchangeRate" maxlength="100" name="ExchangeRate" data-ng-model="objUserSalary.ExchangeRate" ng-blur="setPrice()" ng-bind="objUserSalary" placeholder="ExchangeRate">
                                                            @*<span class="errorMsg" ng-show="submitted && FormAccountEntryInfo.ExchangeRate.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;ExchangeRate is required.</span>*@
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group" data-ng-if="objUserSalary.CurrencyData.Value != 1">
                                                            <label for="Amount">Amount<span class="text-danger">*</span> </label>
                                                            <input type="text" class="form-control" id="Amount" name="Amount" ng-blur="setPrice()" data-ng-model="objUserSalary.Amount" />
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="INRAmount">INR Amount:<span class="text-danger">*</span> </label>
                                                            <div data-ng-if="objUserSalary.CurrencyData.Value != 1">
                                                                <input type="text" class="form-control" id="INRAmount" maxlength="100" readonly name="INRAmount" data-ng-model="objUserSalary.INRAmount" ng-bind="objUserSalary" placeholder="INRAmount">
                                                            </div>
                                                            <input data-ng-show="objUserSalary.CurrencyData.Value == 1" type="text" class="form-control" id="INRAmount" maxlength="100" name="INRAmount" data-ng-model="objUserSalary.INRAmount" ng-bind="objUserSalary" placeholder="INRAmount">
                                                            @*<span class="errorMsg" ng-show="submitted && FormAccountEntryInfo.INRAmount.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;INRAmount is required.</span>*@
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-sm-12">
                                                        <table class="table table-hover">
                                                            <thead>
                                                                <tr>
                                                                    <th>Sr.</th>
                                                                    <th>Salary Head</th>
                                                                    <th>Currency</th>
                                                                    <th>Amount</th>
                                                                    <th>Action</th>
                                                                    <th>
                                                                        <div class="pull-right">
                                                                            <button type="button" class="btn btn-default btn-primary btn-icon" data-ng-click="CreateUpdateSalary(objUserSalary,objUserSalary.CurrencyData.Value)"><i class="fa fa-save"></i> Add Salary</button>
                                                                        </div>
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr data-ng-repeat="list in objUser.UserSalDetails">
                                                                    <td>
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td>
                                                                        {{list.SalaryHead}}
                                                                    </td>
                                                                    <td>
                                                                        {{list.Currency}}
                                                                    </td>
                                                                    <td>
                                                                        {{list.INRAmount}}
                                                                        @*<input type="text" class="form-control" data-ng-model="list.Value" name="$index" />*@
                                                                    </td>
                                                                    <td>
                                                                        <a href="#" class="btn btn-xs default btn-editable" data-tooltip="Edit" data-ng-click="EditSalary(list,$index)" data-tooltip-placement="top" data-tooltip-trigger="mouseenter"><i class="fa fa-pencil pr2"></i></a>
                                                                        <a href="#" class="btn btn-danger btn-xs" data-final-confirm-box="" data-callback="DeleteSalary(list,$index)" data-message="Are you sure want to delete?" data-tooltip="Delete" data-tooltip-placement="top" data-tooltip-trigger="mouseenter"><i class="fa fa-trash pr2"></i></a>
                                                                    </td>
                                                                </tr>
                                                                <tr data-ng-show="objUser.UserSalDetails.length <= 0">
                                                                    <td colspan="4"> <span class="text-danger text-center"> Record not found.</span> </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'AllotmentDetails')">Back</a>
                                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'BasicDetails',objUser)">Save & Continue</a>
                                        </div>

                                        <div id="BasicDetails" class="panel panel-primary tabcontent">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="UserCode">Employee Code:</label>
                                                            <input type="text" class="form-control" maxlength="50" id="UserCode" name="UserCode" data-ng-model="objUser.UserCode" ng-bind="objUser" placeholder="User Code" tabindex="1" readonly>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="Name">First Name:<span class="text-danger">*</span> </label>
                                                            <input type="text" class="form-control" id="EmpName" name="EmpName" maxlength="100" data-ng-model="objUser.Name" ng-bind="objUser" placeholder="Employee Name" tabindex="2" required>
                                                            <span class="errorMsg" ng-show="submitted && FormUserInfo.EmpName.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Employee Name is required.</span>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="FatherName">Middle Name:<span class="text-danger">*</span> </label>
                                                            <input type="text" class="form-control" id="FatherName" name="FatherName" maxlength="100" data-ng-model="objUser.FatherName" ng-bind="objUser" placeholder="Father Name" required tabindex="3">
                                                            <span class="errorMsg" ng-show="submitted && FormUserInfo.FatherName.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Father's Name is required.</span>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label for="Photo" class="col-sm-3 control-label">Photo: <span class="text-danger">*</span></label>
                                                            <div class="col-sm-4">
                                                                <img onerror="this.src = '../../../../UploadImages/UserImage/user.png'" data-ng-src="{{!tempImagePath || tempImagePath == ''  ? '/@UserImagePath/'+ objUser.Photo : tempImagePath}}" data-ng-if="objUser.Photo != '' && objUser.Photo != null" class="img-responsive" tabindex="4" />
                                                                <div data-type="file" class="btn btn-primary" data-ngf-select ng-model="objUser.Photo" data-ngf-change="uploadImgFile($files)" data-name="Photo" data-ngf-accept="'*.jpg,*.jpeg,*.gif,*.png'" style="width: 100%; margin-top: 10px;">
                                                                    Photo Upload
                                                                </div>
                                                                <span class="errorMsg" ng-show="submitted && objUser.Photo == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Image is required.</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
