
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div ng-controller="TaskStatusController">

    <div class="row">
        <div class="col-md-12">
            <div class="portlet light bordered">
                <div class="portlet-body">
                    <div>
                        <div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <elaunchgrid data-grid-obj="gridObj" mode="TaskStatusMaster" title="TaskStatus List" onaddnew="AddTaskStatus()" set-fn="setDirectiveRefresh(theDirFn)"></elaunchgrid>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <!-- END PORTLET-->
        </div>
    </div>
</div>

<script type="text/ng-template" id="unitModalContent.html">
    <ng-include src="'@Url.Action("AddTaskStatus", "TaskStatus")'"></ng-include>
</script>

@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/TaskStatus")
}





