<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">Manage Buyer Contact Detail</h3>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="box">
                    <div class="box-body no-padding">
                        <form accessible-form role="form" name="FormBuyerContactDetailForm" id="FormBuyerContactDetailForm" ng-class="{'submitted': submitted}" ng-submit="submitted=true" novalidate>
                            <div class="modal-body">

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="ContactPerson">Contact Person: <span class="text-danger">*</span> </label>
                                            <input type="text" class="form-control" id="ContactPerson" name="ContactPerson" data-ng-model="objBuyerContact.ContactPerson" placeholder="Contact Person" maxlength="100" required>
                                            <span class="errorMsg" ng-show="submitted && FormBuyerContactDetailForm.ContactPerson.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Contact Person is required.</span>
                                            @*<div data-ng-show="FormBuyerContactDetailForm.ContactPerson.$dirty && FormBuyerContactDetailForm.ContactPerson.$invalid" class="label label-danger">
                                                    Invalid: <span data-ng-show="FormBuyerContactDetailForm.ContactPerson.$error.required">Tell us Contact Person.</span>
                                                </div>*@
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="Surname">Surname: <span class="text-danger">*</span> </label>
                                            <input type="text" class="form-control" id="Surname" name="Surname" data-ng-model="objBuyerContact.Surname" placeholder="Surname" maxlength="100" required>
                                            <span class="errorMsg" ng-show="submitted && FormBuyerContactDetailForm.Surname.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;Surname is required.</span>
                                            @*<div data-ng-show="FormBuyerContactDetailForm.Surname.$dirty && FormBuyerContactDetailForm.Surname.$invalid" class="label label-danger">
                                                    Invalid: <span data-ng-show="FormBuyerContactDetailForm.Surname.$error.required">Tell us Surname.</span>
                                                </div>*@
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="DesignationId" class="control-label">Designation: </label>
                                            <elaunch-autocomplete min-chars="0" mode="DesignationMaster" id="Designation" isdisplay="true"
                                                                  placeholder="Enter Designation"
                                                                  value-changed="callback(value)" ng-model="objBuyerContact.DesignationData">
                                            </elaunch-autocomplete>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="MobileNo">Mobile No.: <span class="text-danger">*</span></label>
                                            @*<input type="text" class="form-control" id="MobileNo" name="MobileNo" data-ng-model="objBuyerContact.MobileNo" placeholder="Mobile No"  >*@
                                            <elaunch-mobile ng-model="teliphone" placeholder="Enter Mobile Number" telcode-array="telCodeData"></elaunch-mobile>
                                            <span class="errorMsg" ng-show="submitted && (FormBuyerContactDetailForm.teliphone.$error.required || teliphone == null || teliphone == '') "><i class="fa fa-exclamation-triangle"></i> &nbsp;Mobile No is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="Email">Email : <span class="text-danger">*</span></label>
                                            @*<input type="text" class="form-control" id="Email" name="Email" data-ng-model="objBuyerContact.Email" placeholder="Email"  >*@
                                            <elaunch-Email ng-model="mail" telcode-array="emailCodeData"></elaunch-Email>
                                            <span class="errorMsg" ng-show="submitted && (FormBuyerContactDetailForm.mail.$error.required || mail == null || mail == '')"><i class="fa fa-exclamation-triangle"></i> &nbsp;Email ID is required.</span>
                                        </div>
                                    </div>
                                </div>



                                @*<div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label for="SkypeId">Skype: </label>
                                                <input type="text" class="form-control" id="SkypeId" name="SkypeId" data-ng-model="objBuyerContact.SkypeId" placeholder="Skype ID" maxlength="50" >
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label for="QQCode">QQCode: </label>
                                                <input type="text" class="form-control" id="QQCode" name="QQCode" data-ng-model="objBuyerContact.QQCode" placeholder="QQCode" maxlength="50" >
                                            </div>
                                        </div>
                                    </div>*@
                            </div>

                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-click="teliphone != '' && mail != '' && FormBuyerContactDetailForm.$valid && CreateUpdate(objBuyerContact)"> <i class="fa fa-save"></i>Submit</button>
                                <button type="button" class="btn btn-default" data-ng-click="close()">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


