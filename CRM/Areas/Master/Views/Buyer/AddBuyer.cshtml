                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="Chat">Chat Details : @*<span class="text-danger">*</span>*@</label>
                                            <elaunch-chat ng-model="chat" chatcode-array="chatCodeData"></elaunch-chat>
                                            @*<span class="errorMsg" ng-show="Contactsubmitted && (chat == null || chat == '') "><i class="fa fa-exclamation-triangle"></i> &nbsp;Chat is required.</span>*@
                                        </div>
                                    </div>                                   
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <table class="table table-hover">
                                            <thead>
                                                <tr>
                                                    <th>Contact Person</th>
                                                    <th>Designation</th>
                                                    <th>Mobile</th>
                                                    <th>Chat</th>
                                                    <th>Email</th>
                                                    <th>Action</th>
                                                    <th><button type="button" name="name" data-ng-click="AddBuyerContactDetail(objBuyerContact)" tabindex="5" class="btn btn-primary btn-sm"><i class="fa fa-plus"></i> Save Contact</button></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr data-ng-repeat="list in objBuyer.BuyerContactDetails | filter: statusFilter">
