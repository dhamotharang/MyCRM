                                                    <label for="cmpnyFaxNo" class="control-label">Company Main Fax No: </label>
                                                    <elaunch-mobile ng-model="cmpnyFaxNo" placeholder="Enter Fax Number" telcode-array="cmpnyFaxCodeData" ng-required="false"></elaunch-mobile>
                                                </div>
