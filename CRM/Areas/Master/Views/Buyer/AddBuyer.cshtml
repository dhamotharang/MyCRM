                                                                  value-changed="callback(value)" ng-model="objBuyerAddress.CityData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="Addresssubmitted && objBuyerAddress.CityData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Please select District.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="PinCode" class="control-label">Pincode: <span class="text-danger">*</span> </label>
                                            <input type="text" class="form-control" oninput="this.value = this.value.replace(/[^0-9.]/g, ''); this.value = this.value.replace(/(\..*)\./g, '$1');" id="PinCode" name="PinCode" data-ng-model="objBuyerAddress.PinCode" placeholder="Pincode" maxlength="50" required>
                                            <span class="errorMsg" ng-show="Addresssubmitted && objBuyerAddress.PinCode==''"><i class="fa fa-exclamation-triangle"></i> &nbsp;PinCode is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label for="Address" class="control-label">Address: <span class="text-danger">*</span> </label>
                                            <textarea class="form-control addressbox" id="Address" name="Address" data-ng-model="objBuyerAddress.Address" placeholder="Address" rows="2" cols="60" style="max-width: 100%;" required></textarea>
                                            <span class="errorMsg" ng-show="Addresssubmitted && objBuyerAddress.Address==''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Address is required.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="WebAddress" class="control-label">Telephone No: @*<span class="text-danger">*</span>*@</label>
                                            <elaunch-mobile ng-model="addreteliphone" placeholder="Enter Telephone Number" telcode-array="addreteliphoneData" ng-required="false"></elaunch-mobile>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="FaxNo" class="control-label">Fax No: </label>
                                            <elaunch-mobile ng-model="addreFaxNo" placeholder="Enter Fax Number" telcode-array="addreFaxCodeData" ng-required="false"></elaunch-mobile>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="Email" class="control-label">Email Address: </label>
                                            <elaunch-Email ng-model="addressemailid" telcode-array="addressemailiddata" ng-required="false"></elaunch-Email>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="WebAddress" class="control-label">Web Site: @*<span class="text-danger">*</span>*@ </label>
                                            @*<input type="text" class="form-control" id="WebAddress" name="WebAddress" data-ng-model="objBuyerAddress.WebAddress" onkeypress="return check(event)" placeholder="Web Address" maxlength="100">*@
                                            <elaunch-website ng-model="adderwebaddress" websitecode-array="AddWebAddressData"></elaunch-website>
                                            @*<span class="errorMsg" ng-show="addressemailid && (FormBuyerInfo.WebAddress.$error.required || objBuyerAddress.WebAddress == null || objBuyerAddress.WebAddress == '')"><i class="fa fa-exclamation-triangle"></i> &nbsp;Company Name is required.</span>*@
                                        </div>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-sm-12">
                                        <div class="pull-right">
                                            <br />
                                            <button type="button" name="btnAddAddress" data-ng-click="AddBuyerAddressDetail(objBuyerAddress)" class="btn btn-primary btn-sm"><i class="fa fa-plus"></i> Save Address</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <table class="table table-hover">
                                            <thead>
                                                <tr>
                                                    <th>Sr.</th>
                                                    <th>Address</th>
                                                    <th>WeeklyOff</th>
                                                    <th>Telephone</th>
                                                    @*<th>Address Type</th>*@
                                                    <th>Email</th>
                                                    <th>WebAddress</th>
                                                    
                                                    @*<th>Pincode</th>
                                                        <th>Weekly off</th>*@
                                                    <th>Action</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr data-ng-repeat="list in objBuyer.BuyerAddressDetails | filter: statusFilter">
                                                    <td>{{$index+1}}</td>
                                                    <td>{{list.Address || '-'}},{{list.CityName}},{{list.PinCode}},{{list.StateName}},{{list.CountryName}}</td>
                                                    <td>{{list.WeeklyOff || '-' }}</td>
                                                    <td>{{list.Telephone || '-'}}</td>
                                                    <td>{{list.Email || '-'}}</td>
                                                    <td>{{list.WebAddress || '-'}}</td>
                                                    @*<td>{{list.WebAddress || '-'}}</td>
                                                        <td>{{list.WeeklyOff || '-'}}</td>*@
                                                    <td>
                                                        <a href="#" class="btn btn-xs default btn-editable" data-tooltip="Edit" data-ng-click="EditBuyerAddressDetail(list,$index)" data-tooltip-placement="top" data-tooltip-trigger="mouseenter"><i class="fa fa-pencil pr2"></i></a>
                                                        <a href="#" class="btn btn-danger btn-xs" data-final-confirm-box="" data-callback="DeleteBuyerAddressDetail(list,$index)" data-message="Are you sure want to delete?" data-tooltip="Delete" data-tooltip-placement="top" data-tooltip-trigger="mouseenter"><i class="fa fa-trash pr2"></i></a>
                                                    </td>
                                                </tr>
                                                <tr data-ng-show="objBuyer.BuyerAddressDetails.length <= 0">
                                                    <td colspan="8"> <span class="text-danger text-center"> Record not found.</span> </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'MainDetails')">Back</a>
                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'ContactDetails',objBuyer)">Save & Continue</a>
                            <button type="button" class="btn btn-sm btn-info btn-icon pull-right" @*data-ng-if='!addMode'*@
                                    data-ng-click="openTab(event, 'ContactDetails',objBuyer,'final')" tabindex="77">
                                Save&Exit
                            </button>
                        </div>

                        <div id="ContactDetails" class="panel panel-primary tabcontent">
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-6">
