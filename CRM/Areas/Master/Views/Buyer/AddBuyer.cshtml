                                                                  value-changed="callback(value)" ng-model="objBuyerAddress.CountryData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="Addresssubmitted && objBuyerAddress.CountryData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Please select Country.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="StateData" class="control-label">State: <span class="text-danger">*</span></label>
                                            <elaunch-autocomplete min-chars="0" mode="StateMaster" id="State" isdisplay="objBuyerAddress.CountryData.Value!=''"
                                                                  placeholder="Enter State" related-value="objBuyerAddress.CountryData.Value" ng-required="false"
                                                                  value-changed="callback(value)" ng-model="objBuyerAddress.StateData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="Addresssubmitted && objBuyerAddress.StateData.Display == ''"><i class="fa fa-exclamation-triangle"></i> &nbsp;Please select State.</span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="CityData" class="control-label">City/district: <span class="text-danger">*</span></label>
                                            <elaunch-autocomplete min-chars="0" mode="CityMaster" id="City" isdisplay="objBuyerAddress.StateData.Value!=''"
