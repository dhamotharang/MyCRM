                                                                  value-changed="callback(value)" ng-model="objBuyerAddress.addressData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="Addresssubmitted && (objBuyerAddress.addressData.Display == '' || objBuyerAddress.addressData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Please select AddressType.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="WeeklyOff" class="control-label">Weekly off: </label>
                                            <select name="WeeklyOff" id="WeeklyOff" data-ng-model="objBuyerAddress.WeeklyOff" class="form-control input-dark">
                                                <option value=""> ---Select--- </option>
                                                <option value="AllDayWorking"> All Day Working </option>
                                                <option value="Sunday" selected> Sunday </option>
                                                <option value="Monday"> Monday </option>
                                                <option value="Tuesday"> Tuesday </option>
                                                <option value="Wednesday"> Wednesday </option>
                                                <option value="Thursday"> Thursday </option>
                                                <option value="Friday"> Friday </option>
                                                <option value="Saturday"> Saturday </option>
                                                <option value="Saturday&Sunday"> Saturday&Sunday </option>
                                            </select>
