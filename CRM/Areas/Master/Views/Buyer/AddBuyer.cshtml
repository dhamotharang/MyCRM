                                            <label><input type="checkbox" title="Select all" ng-model="objBuyer.IsSupplierSelect">Supplier</label>
                                            <label><input type="checkbox" title="Select all" ng-model="objBuyer.IsVendorSelect">Vendor</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            @*  ng-keypress="CheckValue(objBuyer.CompanyName)"  *@
                                            <label for="CompanyName" class="control-label">Company Full Name: <span class="text-danger">*</span> </label>
                                            <input type="text" first-alphabet class="form-control addressbox" id="CompanyName" name="CompanyName" data-ng-model="objBuyer.CompanyName" onkeypress="return check(event)" placeholder="Company Name" maxlength="100" tabindex="1" ng-blur="GetDuplicationDataInfoByName(objBuyer)" required>
                                            <span class="errorMsg" ng-show="submitted && (FormBuyerInfo.CompanyName.$error.required || objBuyer.CompanyName == null || objBuyer.CompanyName == '')"><i class="fa fa-exclamation-triangle"></i> &nbsp;Company Name is required.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="ContactData" class="control-label">Type of Contact:<span class="text-danger">*</span></label>
                                            <elaunch-autocomplete min-chars="0" mode="AgencyTypeMaster" id="ContactData" isdisplay="true"
                                                                  value-changed="callback(value)" ng-model="objBuyer.ContactData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="submitted && (FormBuyerInfo.CompanyName.$error.required || objBuyer.CompanyName == null || objBuyer.CompanyName == '')"><i class="fa fa-exclamation-triangle"></i> &nbsp;Please select Type of Contact.</span>
                                        </div>
                                    </div>
                                    @*<div class="col-sm-6">
                                            <div class="form-group">
                                                <label for="WebAddress" class="control-label">Company Website:</label>
                                                <elaunch-website ng-model="webaddress" websitecode-array="WebAddressData"></elaunch-website>
                                            </div>
                                        </div>*@
                                </div>

                                @*<div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label for="WebAddress" class="control-label">Company Main Telephone no:</label>
                                                <elaunch-mobile ng-model="cmpnyteliphone" placeholder="Enter Telephone Number" telcode-array="cmpnytelCodeData" ng-required="false"></elaunch-mobile>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label for="Email" class="control-label">Company Main Email address:</label>
                                                <elaunch-Email ng-model="emailid" telcode-array="cmpnyemailCodeData" ng-required="false"></elaunch-Email>
