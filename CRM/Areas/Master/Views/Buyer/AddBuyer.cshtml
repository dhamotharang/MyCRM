                        <div id="AddressDetails" class="panel panel-primary tabcontent">
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="addressData" class="control-label">Type of address: <span class="text-danger">*</span></label>
                                            <elaunch-autocomplete min-chars="0" mode="AddressTypeMaster" id="address" isdisplay="true"
