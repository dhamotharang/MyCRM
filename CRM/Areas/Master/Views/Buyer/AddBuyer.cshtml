                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'AddressDetails')">Back</a>
                            <a class="btn btn-sm btn-info btn-icon" data-ng-click="openTab(event, 'TaxDetails',objBuyer)">Save & Continue</a>
