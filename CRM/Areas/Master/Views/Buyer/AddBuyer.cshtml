                                            <textarea class="form-control" tabindex="3" id="Remark" name="Remark" data-ng-model="objBuyer.Remark" placeholder="Remark" rows="3" cols="60" style="max-width: 100%;"></textarea>
                                        </div>
                                    </div>
                                </div>
                                @*<div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="InvitationAlert">Invitation Alert :</label>
                                            <div isteven-multi-select
