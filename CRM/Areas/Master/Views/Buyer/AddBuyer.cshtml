                                    data-ng-click="openTab(event, 'TaxDetails',objBuyer,'final')" tabindex="77">
                                Save&Exit
                            </button>
                        </div>

                        <div id="TaxDetails" class="panel panel-primary tabcontent">
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="Tax">Tax Name:@*<span class="text-danger">*</span>*@ </label>
                                            <elaunch-autocomplete min-chars="0" mode="LicenseMaster" id="Tax" isdisplay="true"
                                                                  placeholder="Enter Tax" tabindex="7" ng-required="false"
                                                                  value-changed="callback(value)" ng-model="objBuyerTax.LicenseData">
                                            </elaunch-autocomplete>
                                            <span class="errorMsg" ng-show="Taxsubmittedd && (objBuyerTax.LicenseData.Display =='' || objBuyerTax.LicenseData.Display == null)"><i class="fa fa-exclamation-triangle"></i> &nbsp;Tax is required.</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
