<div class="row">
    <div class="col-sm-12">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" data-ng-click="close()">&times;</button>
            <h3 class="modal-title" id="modal-title">Manage License Information</h3>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="box">
                    <div class="box-body no-padding">
                        <form accessible-form role="form" name="FormLicenseInfo" id="FormLicenseInfo" ng-class="{'submitted': submitted}" ng-submit="submitted=true" novalidate>
                            <fieldset ng-disabled="isClicked">
                                <div class="modal-body">
                                    <div class="form-group">
                                        <label for="LicenseName">LicenseName<span class="text-danger">*</span></label>
                                        <input type="text" class="form-control" id="LicenseName" name="LicenseName" data-ng-model="objLicense.LicenseName" placeholder="LicenseName" required maxlength="100" />
                                        <span class="errorMsg" ng-show="submitted && FormLicenseInfo.LicenseName.$error.required"><i class="fa fa-exclamation-triangle"></i> &nbsp;License Name is required.</span>
                                    </div>
                                </div>
                            </fieldset>
                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="id <= 0 && isClicked == false" data-ng-click="FormLicenseInfo.$valid && CreateUpdate(objLicense)"><i class="fa fa-save"></i>Save</button>
                                <button type="submit" class="btn btn-default btn-info btn-icon icon-left" data-ng-if="id > 0 && isClicked == false" data-ng-click="FormLicenseInfo.$valid && CreateUpdate(objLicense)"><i class="fa fa-edit"></i>Update</button>
                                <button type="button" class="btn btn-default" data-ng-click="close()">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>





