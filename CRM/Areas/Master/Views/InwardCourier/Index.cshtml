@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div data-ng-controller="InwardCourierCtrl">

    <div class="row">
        <div class="col-md-12">
            <div class="portlet light bordered">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <elaunchgrid data-grid-obj="gridObj" mode="InwardCourierMaster" title="Inward Courier List" set-fn="setDirectiveRefresh(theDirFn)" onaddnew="Add(objInwardCourier)"></elaunchgrid>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/ng-template" id="InwardCourierModal.html">
    <ng-include src="'@Url.Action("AddCourier", "InwardCourier")'"></ng-include>
</script>


@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/InwardCourier")
}