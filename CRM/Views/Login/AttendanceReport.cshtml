
@{
    ViewBag.Title = "AttendanceReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div ng-controller="LoginController">



    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN PORTLET-->
            <div class="portlet light bordered">

                <div class="portlet-body">
                    <div>
                        <div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <elaunchgrid data-grid-obj="gridatndObj" is-add="false" mode="AttendanceMaster" set-fn="setDirectiveRefresh(theDirFn)" title="Attendance List"></elaunchgrid>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END PORTLET-->
        </div>
    </div>
</div>
@section Scripts
{
    @Scripts.Render("~/bundles/ElaunchControls")
    @Scripts.Render("~/bundles/Login")
}